{
    "name": "nerow/laravel-logs",
    "description": "Simple log package for Laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Nerow\\Logs\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nerowin",
            "email": "nerow.op@gmail.com"
        }
    ],
    "require": {
        "nerow/laravel-services": "dev-main"
    },
    
    "scripts": {
        "post-package-install": [
            "@prepare"
        ],
        "post-package-update": [
            "@prepare"
        ],
        "prepare": "@php artisan vendor:publish --provider=Nerow\\Logs\\Providers\\LogServiceProvider"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nerow\\Logs\\Providers\\LogServiceProvider"
            ],
            "aliases": {
                "Nerow\\Services\\LogService": "Nerow\\Logs\\LogService",
                "Nerow\\Traits\\HasLogs": "Nerow\\Logs\\Traits\\HasLogs",
                "Nerow\\Traits\\ServiceLogger": "Nerow\\Logs\\Traits\\ServiceLogger",
                "Nerow\\Models\\Log": "Nerow\\Logs\\Models\\Log"
            }
        }
    },
    "repositories": {
        "laravel-services": {
            "type": "vcs",
            "url": "https://github.com/Nerowin/laravel-services.git"
        }
    }
}
